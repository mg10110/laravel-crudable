{
    "name": "mindz-team/laravel-crudable",
    "description": "This is a package to create crudable controllers in no time",
    "authors": [
        {
            "name": "roman.szymanski",
            "email": "r.szymanski@mindz.pl"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mindz\\LaravelCrudable\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindz\\LaravelCrudable\\LaravelCrudableServiceProvider"
            ]
        }
    }
}
